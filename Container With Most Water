


// O(n^2) time complexity, and exceed the time limit.
public class Solution {
    public int maxArea(int[] height) {
        int l=height.length;
        int area=0;
        int maxArea=0;
        for(int i=0;i<l;i++){
            for(int j=1;j<l;j++){
                area=(Math.min(height[i],height[j]))*(j-i);
                maxArea=Math.max(maxArea,area);
            }
        }
        return maxArea;
    }
}
